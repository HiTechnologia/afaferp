<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="act_approval_settings" model="ir.actions.act_window">
		<field name="name">Approval Settings</field>
		<field name="res_model">approval.settings</field>
		<field name="view_mode">list,form</field>
		<field name="path">approval_settings</field>
	</record>

	<record id="act_approval_log" model="ir.actions.act_window">
		<field name="name">Approval Log</field>
		<field name="res_model">approval.log</field>
		<field name="view_mode">list,pivot,graph</field>
		<field name="path">approval_log</field>
	</record>	

	<record id="act_approval_config" model="ir.actions.act_window">
		<field name="name">Approval Status</field>
		<field name="res_model">approval.config</field>
		<field name="view_mode">list,form</field>
		<field name="path">approval_config</field>
		<field name="context">{'create' : False}</field>
	</record>	

	<record id="act_approval_buttons" model="ir.actions.act_window">
		<field name="name">Approval Button</field>
		<field name="res_model">approval.buttons</field>
		<field name="view_mode">list,form</field>
		<field name="path">approval_buttons</field>
		<field name="context">{'create' : False}</field>
	</record>	

	<record id="act_approval_automation" model="ir.actions.act_window">
		<field name="name">Approval Automation</field>
		<field name="res_model">approval.automation</field>
		<field name="view_mode">list,form</field>
		<field name="path">approval_automation</field>
		<field name="context">{'create' : False}</field>
	</record>	

</odoo>